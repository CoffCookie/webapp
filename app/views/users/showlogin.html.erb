<h1>login</h1>
<%= flash[:alert] %>
<%= form_with( url: users_login_path, local: true) do |form| %>
  <div class="field">
    <%= form.label :name, "名前" %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :password, "パスワード" %>
    <%= form.password_field :password %>
  </div>

  <div class="actions">
    <%= form.submit :login %>
  </div>
<% end %>
